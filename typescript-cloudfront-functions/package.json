{
  "name": "typescript-cloudfront-functions",
  "version": "1.0.0",
  "description": "CloudFront Functions written in TypeScript",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest",
    "clean": "rm -rf dist node_modules"
  },
  "files": [
    "dist/*.js"
  ],
  "keywords": [],
  "author": "Asher Gilani",
  "license": "ISC",
  "jest": {
    "testMatch": [
      "**/**.test.ts"
    ],
    "transform": {
      ".(js|ts)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx|json)$",
      "package.json"
    ]
  },
  "devDependencies": {
    "@types/aws-cloudfront-function": "^1.0.2",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
